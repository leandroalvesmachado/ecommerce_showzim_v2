<h1>Olá</h1>
<% if user_signed_in? %>
  <h1>Bem vindo</h1>
  <%= button_to "logout", destroy_user_session_path, method: :delete %>
<% else %>
  <h2>Você não esta logado</h2>
  <%= link_to "Sign up", new_user_registration_path %><br />
  <%= link_to "Log in", new_user_session_path %><br />
<% end %>
